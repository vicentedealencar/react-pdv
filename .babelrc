{
  "presets": [
    "react"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread"
  ],
  "env": {
    "commonjs": {
      "presets": [
        ["env", { "loose": true }]
      ]
    },
    "es": {
      "presets": [
        ["env", { "modules": false, "loose": true }]
      ]
    },
    "rollup": {
      "presets": [
        ["env", { "modules": false, "loose": true }]
      ],
      "plugins": [
        "external-helpers"
      ]
    }
  }
}
