{
  "name": "react-pdv",
  "version": "0.0.29",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "start": "kit ./library/examples/ --mode library",
    "build": "babel src -d dist",
    "old-build": "cross-env NODE_ENV=production rollup -c",
    "prepare": "yarn build",
    "now-start": "cross-env NODE_ENV=production x0 dev library/App.js --port 5757",
    "deploy": "now",
    "test": "jest --notify --colors",
    "test:update": "yarn test --updateSnapshot",
    "test:bail": "yarn test --bail",
    "test:format": "npm-run-all --serial test format",
    "format": "prettier-eslint --write **/*.js !**/dist/**",
    "clean": "rimraf ./dist/*"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vicentedealencar/react-pdv.git"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn test:format"
    }
  },
  "prettier": {
    "proseWrap": "never",
    "singleQuote": true,
    "semi": false
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|react|react-pdv)/)",
      "**/*.test.js"
    ]
  },
  "x0": {
    "config": "library/x0.config.js"
  },
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "@compositor/kit-cli": "^1.0.37",
    "@compositor/kit-snapshot": "^1.0.24",
    "@compositor/x0": "^5.0.8",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.6",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.9.1",
    "husky": "^1.0.0-rc.8",
    "jest": "^23.1.0",
    "npm-run-all": "^4.1.3",
    "prettier-eslint-cli": "^4.7.1",
    "react": "^16.4.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.60.7",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0"
  },
  "dependencies": {
    "numeral": "^2.0.6",
    "rollup-plugin-peer-deps-external": "^2.1.1"
  }
}
